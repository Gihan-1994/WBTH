{
  "name": "wbth",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "migrate": "yarn -C packages/prisma prisma migrate deploy",
    "seed": "yarn -C packages/prisma prisma db seed"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/config/*"
  ],
  "dependencies": {
    "@prisma/client": "^7.2.0"
  }
}