{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "test": {},
    "typecheck": {},
    "migrate": {
      "dependsOn": ["@repo/prisma#build"],
      "cache": false
    },
    "seed": {
      "dependsOn": ["@repo/prisma#build", "migrate"],
      "cache": false
    }
  }
}
